{
    "name": "haphan/php-rage4dns",
    "description": "Rage4 DNS API PHP 5.3+ Library",
    "keywords" : ["Rage4", "Rage4 DNS", "DNS", "API"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ha Phan",
            "email": "thanhha.work@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl" : "*",
        "symfony/console": "~2.4",
        "guzzle/guzzle": "~3.8",
        "symfony/yaml" : "~2.4"
    },

    "autoload": {
        "psr-4": {
            "Haphan\\Rage4DNS\\": "src/Haphan/Rage4DNS"
        }
    },

    "config": {
        "bin-dir": "bin"
    },

    "bin" : ["bin/rage4dns"],

    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
